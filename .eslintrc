{
  "extends": [
    "react-app",
    "wesbos",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "no-console": 2,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "src/"]
      }
    }
  },
  "plugins": ["jest"],
  "env": {
    "jest": true
  }
}
